                                                            <td class="product-remove">
                                                                <a href="#" data-bs-toggle="modal" data-bs-target="#deleteModal" data-itemId="<%= item._id %>">
                                                                    <i class="dlicon ui-1_simple-remove"></i>
                                                                </a>
                                                            </td>
