<script>
    
    var deleteModal = document.getElementById('deleteModal');
        deleteModal.addEventListener('show.bs.modal', function (event) {
            var button = event.relatedTarget;
            var itemId = button.getAttribute('data-itemId'); 
            var confirmDelete = deleteModal.querySelector('#confirmDelete');
            confirmDelete.href = '/user/remove-from-cart/' + itemId; 
        });

        document.getElementById('cartForm').onsubmit = async function(event) {
    event.preventDefault();

    const formData = new FormData(this);
    const response = await fetch(this.action, {
        method: 'POST',
        body: formData
    });

    const result = await response.json();

    if (!result.success) {
        showToast(result.message)
    } else {
        window.location.href = '/user/checkout-order/';
    }
};

function showToast(message) {
    const toast = document.getElementById('toast');
    const toastMessage = document.getElementById('toastMessage');
    toastMessage.textContent = message;
    toast.style.display = 'block';

    setTimeout(() => {
        toast.style.display = 'none';
    }, 3000);
}
        function decreaseQuantity(itemId) {
    console.log(`Decreasing quantity for item: ${itemId}`);
    const qtyInput = document.getElementById(`qtybutton-${itemId}`);
    console.log(`qtyInput:`, qtyInput); // Check if qtyInput is null
    if (qtyInput) {
        let quantity = parseInt(qtyInput.value);
        if (quantity > 1) { // Prevent quantity from going below 1
            qtyInput.value = quantity - 1; 
            updateCart(itemId, qtyInput.value); // Call updateCart after changing quantity
        } else {
            console.warn(`Quantity cannot be less than 1 for item: ${itemId}`);
        }
    } else {
        console.error(`Element with ID qtybutton-${itemId} not found.`);
    }
}

function increaseQuantity(itemId) {
    console.log(`Increasing quantity for item: ${itemId}`);
    const qtyInput = document.getElementById(`qtybutton-${itemId}`);
    console.log(`qtyInput:`, qtyInput); // Check if qtyInput is null
    if (qtyInput) {
        let quantity = parseInt(qtyInput.value);
        if(quantity<5){
        qtyInput.value = quantity + 1; 
        updateCart(itemId, qtyInput.value)
         } else {
            console.warn(`Quantity cannot be more than 5 for item: ${itemId}`);
        }  
        } else {
        console.error(`Element with ID qtybutton-${itemId} not found.`);
    }
}       

function updateCart(itemId, newQuantity) {
    fetch('/user/cart/update', {
        method: 'POST',
        headers: {
            'Content-Type': 'application/json',
        },
        body: JSON.stringify({ itemId, quantity: newQuantity }),
    })
    .then(response => response.json())
    .then(data => {
        if (data.success) {
            console.log('Cart updated successfully');
            // document.querySelector('#cartMessage').textContent = 'Cart updated!';
            
            // Update the total cost in the DOM for the item
            document.querySelector(`#total-${itemId}`).textContent = 
                '₹' + (data.updatedPrice).toFixed(2);
            
            // Optionally, update the overall cart total
            document.querySelector('#cartTotal').textContent = 
                '₹' + (data.cartTotal).toFixed(2)

                document.querySelector('#cartTotalQuantity').textContent =  data.cartQuantity
        } else {
            console.error('Failed to update cart:', data.message);
            document.querySelector('#cartMessage').textContent = 'Error updating cart.';
        }
    })
    .catch(error => {
        console.error('Error:', error);
        document.querySelector('#cartMessage').textContent = 'An error occurred.';
    });
}

