                                                                                    <% if (order.orderStatus !== 'delivered') { %>
                                                                                        <a href="/user/account/cancel-order?id=<%= order._id %>">
                                                                                            <button class="btn btn-danger btn-sm" type="button">Cancel Order</button>
                                                                                        </a>
