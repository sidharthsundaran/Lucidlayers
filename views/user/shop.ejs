<script>
   function addToWishlist(productId) {
    console.log("hi");
    
    $.ajax({
        type: 'POST',
        url: '/user/add-to-wishlist',
        contentType: 'application/json',
        data: JSON.stringify({ productId: productId }),
        success: function(response) {
            showToast(response.message, 'success');
        },
        error: function(xhr, status, error) {
            showToast('Error adding to wishlist.', 'error');
            console.error(xhr.responseText);
        }
    });
}

function removeFromWishlist(productId) {
    $.ajax({
        type: 'POST',
        url: '/user/remove-from-wishlist',
        contentType: 'application/json',
        data: JSON.stringify({ productId: productId }),
        success: function(response) {
            showToast(response.message, 'success');
        },
        error: function(xhr, status, error) {
            showToast('Error removing from wishlist.', 'error');
            console.error(xhr.responseText);
        }
    });
}

async function toggleWishlist(productId) {
    const isInWishlist = document.getElementById(`wishlist-toggle-${productId}`).querySelector('i').classList.contains('fa-heart');

    try {
        const response = await fetch(isInWishlist ? '/user/remove-from-wishlist' : '/user/add-to-wishlist', {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json'
            },
            body: JSON.stringify({ productId })
        });

        const result = await response.json();
        showToast(result.message, response.ok ? 'success' : 'error');

        const icon = document.getElementById(`wishlist-toggle-${productId}`).querySelector('i');
        const span = document.getElementById(`wishlist-toggle-${productId}`).querySelector('span');

        if (isInWishlist) {
            icon.classList.remove('fa-heart');
            icon.classList.add('fa-heart-o');
            span.textContent = 'Add to Wishlist';
        } else {
            icon.classList.remove('fa-heart-o');
            icon.classList.add('fa-heart');
            span.textContent = 'Remove from Wishlist';
        }
    } catch (error) {
        console.error('Error:', error);
        showToast('An error occurred while updating the wishlist.', 'error');
    }
}
    function showToast(message, type) {
        const toast = $('<div class="toast"></div>').text(message);
        if (type === 'success') {
            toast.addClass('toast-success');
        } else {
            toast.addClass('toast-error');
        }

        $('body').append(toast);
        toast.fadeIn(400).delay(3000).fadeOut(400, function() {
            $(this).remove();
        });
    }

    $(function() {
        // Initialize the slider
        $("#slider-range").slider({
            range: true,
            min: 500, 
            max: 5000,
            values: [1000, 3000], // Default values
            slide: function(event, ui) {
                $("#amount").val("₹" + ui.values[0] + " - ₹" + ui.values[1]);
            },
            stop: function(event, ui) {
                $("#amount").val("₹" + ui.values[0] + " - ₹" + ui.values[1]);
            }
        });
    
        $("#amount").val("₹" + $("#slider-range").slider("values", 0) + " - ₹" + $("#slider-range").slider("values", 1));
    
        $("#filter-price").on("click", function() {
            const minPrice = $("#slider-range").slider("values", 0);
            const maxPrice = $("#slider-range").slider("values", 1);
            
            window.location.href = `/user/shop?price=${minPrice}-${maxPrice}&page=1&limit=<%= limit %>&sort=<%= sortOption %>`;
