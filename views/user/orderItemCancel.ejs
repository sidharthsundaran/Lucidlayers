                                                <% order[0].items.forEach(item => { %>
                                                    <tr>
                                                        <td>
                                                            <div class="d-flex align-items-center">
                                                                <img class="thumbnail me-2" src="/uploads/<%= item.productId.images[0] %>" alt="" class="item-image">
                                                                <span class="item-name"><%= item.productName %></span>
                                                            </div>
                                                        </td>
                                                        <td><%= item.size %></td>
                                                        <td>$<%= item.price.toFixed(2) %></td>
                                                        <td><%= item.quantity %></td>
                                                        <td>$<%= (item.price * item.quantity).toFixed(2) %></td>
                                                        <td><%= item.orderStatus %></td>
                    
                                                    
                                                    </tr>
                                                    <tr id="item-details-<%= order[0].orderNo %>-<%= item.productId._id %>" class="item-details" style="display: none;">
