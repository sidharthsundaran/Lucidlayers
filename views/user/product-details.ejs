}

.product-details-quality button {
    width: 30px;
    height: 30px;
    background-color: transparent;
    color: #333;
    border: none;
    font-size: 20px;
    cursor: pointer;
    transition: color 0.3s;
}

.product-details-quality button:hover {
    color: #555; /* Darker shade on hover */
}

.product-details-quality input[type="text"] {
    width: 40px;
    text-align: center;
    font-size: 16px;
    background: transparent;
    border: none;
    outline: none;
    color: #333;
}

.product-details-quality input[type="text"]:focus {
    color: #000;
}

/* Optional: Increase button hover effect for minimalism */
.product-details-quality button:active {
    color: #000;
}

.product-details-cart button {
    background-color: #000;
    color: #fff;
    border: none;
    padding: 10px 15px;
    border-radius: 5px;
    cursor: pointer;
    font-size: 14px;
    transition: background-color 0.3s;
}

.product-details-cart button:hover {
    background-color: #444;
}

.product-details-wishlist, .product-details-compare {
    font-size: 18px;
    color: #555;
    padding: 5px;
    transition: color 0.3s;
}

.product-details-wishlist:hover, .product-details-compare:hover {
    color: #e74c3c; /* Change color on hover for effect */
}
    .size-option {
        color: black; /* Default color */
        cursor: pointer;
    }

    .size-option.selected {
        color: blue; /* Color for the selected size */
