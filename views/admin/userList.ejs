            <section class="content-main">
                <div class="content-header">
                    <h2 class="content-title">Users list</h2>
                </div>
                <div class="card mb-4">
                    <header class="card-header">
                        <div class="row gx-3">
                            <div class="col-lg-4 col-md-6 me-auto">
                                <input type="text" placeholder="Search..." class="form-control">
                            </div>
                            <div class="col-lg-2 col-md-3 col-6">
                                <select class="form-select">
                                    <option>Status</option>
                                    <option>Active</option>
                                    <option>Disabled</option>
                                    <option>Show all</option>
                                </select>
                            </div>
                            <div class="col-lg-2 col-md-3 col-6">
                                <select class="form-select">
                                    <option>Show 20</option>
                                    <option>Show 30</option>
                                    <option>Show 40</option>
                                </select>
                            </div>
                        </div>
                    </header> <!-- card-header end// -->
                    <div class="card-body">
                        <div class="table-responsive">
                            <table class="table table-hover">
                                <thead>
                                    <tr>
                                        <th>user</th>
                                        <th>Email</th>
                                        <th>Status</th>
                                        <th>Registered</th>
                                        <th>Number</th>
                                        <th class="text-end"> Action </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% if(user.length > 0) { %>
                                        <% for( let i=0;i<user.length;i++){ %>
                                            <tr>
                                                <td width="40%">
                                                    <a href="#" class="itemside">
                                                        <div class="left">
                                                            <img src="/admin/assets/imgs/people/avatar5.jpg" class="img-sm img-avatar" alt="Userpic">
                                                        </div>
                                                        <div class="info pl-3">
                                                            <h6 class="mb-0 title"><%= user[i].name %> </h6>
                                                        </div>
                                                    </a>
                                                </td>
                                                <td><%= user[i].email %></td>
                                                <td><span class="badge rounded-pill alert-success">Active</span></td>
                                                <td><%= user[i].dateJoined %></td>
                                                <td><%= user[i].phone %></td>
                                                <td class="text-end">
                                                    <% if( user[i].blocked ==false){ %>
                                                        <button class="btn btn-sm btn-danger" data-bs-toggle="modal" data-bs-target="#blockModal" data-userId="<%= user[i]._id %>">Block</button>
                                                        <% } if(user[i].blocked==true) { %>
                                                            <button class="btn btn-sm btn-primary" data-bs-toggle="modal" data-bs-target="#unblockModal" data-userId="<%= user[i]._id %>">Unblock</button>
                                                            <% } %>
                                                </td>
                                            </tr>
                                    <% } %>
                                    <% } %>
                                
                                </tbody>
